#!/usr/bin/env bash

function main {
  local shome="${_tunnelblick_home:-"$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"}"
  source "$shome/script/profile"

  case "$(uname -s)" in 
    Darwin)
      if [[ ! -d "/Applications/Tunnelblick.app" ]]; then
        return 0
      fi
      ;;
  esac

  return 1
}

source sub "$BASH_SOURCE" "$@"
